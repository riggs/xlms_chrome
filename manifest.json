{
  "manifest_version": 2,
  "name": "XLMS",
  "author": "http://github.com/riggs",
  "description": "XLMS Chrome App",
  "version": "0.1.0",
  "version_name": "alpha",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "url_handlers": {
    "launch_exercise": {
      "matches": [
        "http://xlms.org/exercise*",
        "http://demo.xlms.advantagelabs.com/exercise*"
      ],
      "title": "Launch Exercise"
    }
  },
  "permissions": [
    "hid",
    {"usbDevices": [
      {"vendorId": 1003, "productId": 8256},
      {"vendorId": 1003, "productId": 8257}
    ]},
    "http://xlms.org/",
    "http://demo.xlms.advantagelabs.com/",
    "power",
    "audioCapture",
    "videoCapture",
    "webview"
  ],
  "webview": {
    "partitions": [
      {
        "name": "plugin",
        "accessible_resources": ["style.css"]
      }
    ]
  }
}